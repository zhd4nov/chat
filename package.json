{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "prestart": "rimraf dist/ && babel ./app --out-dir dist",
    "start": "node dist/index.js",
    "server": "nodemon --ignore 'app/data/*.json' --exec babel-node app/index.js",
    "client": "cd ./client && npm run dev",
    "dev": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "cd client && npm install && npm install --only=dev --no-shrinkwrap && rimraf dist/ && npm run build"
  },
  "engines": {
    "node": "13.8.x",
    "npm": "6.13.x"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/node": "^7.8.7",
    "@babel/preset-env": "^7.9.5",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "morgan": "^1.10.0",
    "nanoid": "^3.1.3",
    "nodemon": "^2.0.3",
    "regenerator-runtime": "^0.13.5",
    "rimraf": "^3.0.2",
    "socket.io": "^2.3.0"
  },
  "devDependencies": {
    "concurrently": "^5.2.0"
  }
}